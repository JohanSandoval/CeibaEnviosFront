{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@core/*": [
        "src/app/core/*"
      ],
      "@core-service/*": [
        "src/app/core/services/*"
      ],
      "@home/*": [
        "src/app/feature/home/*"
      ],
      "@producto/*": [
        "src/app/feature/producto/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@envio/*": [
        "src/app/feature/envio/*"
      ],
      "@ciudad/*":[
        "src/app/feature/ciudad/*"
      ],
      "@usuario/*":[
        "src/app/feature/usuario/*"
      ],
      "@dias-espera/*":[
        "src/app/feature/dias-espera/*"
      ],
      "@costos/*":[
        "src/app/feature/costos/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
